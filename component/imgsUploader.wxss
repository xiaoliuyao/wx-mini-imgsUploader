/* component/imgsUploader.wxss */

.container{
  font-size: 12px;
}
.tishi{
  display: flex;
  align-items: center;
  padding: 6px 14px; 
  color: #ff470e;
  background-color: #f5f5f5;
}
.icon{
  width: 11px;
  height: 15px;
  margin-right: 17px;
}
.box{
  display: flex;
  flex-wrap: wrap;
  padding: 13px 0 52px;
  background-color: #ffffff;
}
.imgbox{
  position: relative;
  width: 25.11%;
  margin-left: 4.11%;
  margin-right: 4.11%;
  margin-bottom: 15px;
  padding-bottom: 25.11%;
  height: 0;
  text-align: center;
}
.car-img{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  vertical-align: bottom;
}
.fengmian{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 21px;
  background: rgba(255, 255, 255, .4);
  line-height: 21px;
  font-size: 12px;
  color: #ffffff;
}

.btn{
  padding: 0 10% 20px;
  background-color: #ffffff;
}
.confirmbtn{
  width: 100%;
  padding: 10px 0;
  text-align: center;
  background-color: #09a4f6;
  color: #ffffff;
  font-size: 15px;
  border-radius: 4px;
}